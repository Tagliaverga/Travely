<h1>Create a new trip</h1>

<%= render 'form', trip: @trip %>


<div class="container">
  <div class="search-form-control form-group m-3">
    <%= form_with url: users_path, method: :get, class: "d-flex" do %>
      <%= text_field_tag :query,
            params[:query],
            class: "form-control",
            placeholder: "Buddy"
        %>
      <%= submit_tag "Buscar", name: "", class: "btn btn-flat" %>
    <% end %>
  </div>
</div>
